{
    "admin": {
        "listen": "0.0.0.0:2019"
    },
    "apps": {
        "http": {
            "servers": {
                "Cloud_Guardian": {
                    "listen": [
                        ":80",
                        ":443"
                    ],
                    "routes": [
                        {
                            "match": [
                                {
                                    "path": [
                                        "/static/*"
                                    ]
                                }
                            ],
                            "handle": [
                                {
                                    "handler": "file_server",
                                    "root": "C:\\Users\\USUARIO\\Desktop\\fct\\FIREWALL\\cloudguardian-deploy\\backend\\staticfiles"
                                }
                            ]
                        },
                        {
                            "match": [
                                {
                                    "path": [
                                        "/ian90/*"
                                    ]
                                }
                            ],
                            "handle": [
                                {
                                    "handler": "static_response",
                                    "body": "Acceso permitido a ian90"
                                }
                            ]
                        },
                        {
                            "match": [
                                {
                                    "path": [
                                        "/api/*"
                                    ]
                                }
                            ],
                            "handle": [
                                {
                                    "handler": "static_response",
                                    "body": "Acceso permitido a /api/*"
                                }
                            ]
                        },
                        {
                            "match": [
                                {
                                    "path": [
                                        "/ian1990/*"
                                    ],
                                    "remote_ip": {
                                        "ranges": [
                                            "222.222.222.22",
                                            "111.111.111.11"
                                        ]
                                    }
                                }
                            ],
                            "handle": [
                                {
                                    "handler": "static_response",
                                    "status_code": 403,
                                    "body": "IP bloqueada por Cloud Guardian"
                                }
                            ]
                        },
                        {
                            "match": [
                                {
                                    "path": [
                                        "/ian1990/practicas"
                                    ]
                                }
                            ],
                            "handle": [
                                {
                                    "handler": "static_response",
                                    "body": "Acceso permitido a /ian1990/practicas"
                                }
                            ]
                        },
                        {
                            "match": [
                                {
                                    "path": [
                                        "/ian1990/estudio"
                                    ]
                                }
                            ],
                            "handle": [
                                {
                                    "handler": "static_response",
                                    "body": "Acceso permitido a /ian1990/estudio"
                                }
                            ]
                        },
                        {
                            "match": [
                                {
                                    "path": [
                                        "/ian1990/cccc"
                                    ]
                                }
                            ],
                            "handle": [
                                {
                                    "handler": "static_response",
                                    "body": "Acceso permitido a /ian1990/cccc"
                                }
                            ]
                        },
                        {
                            "match": [
                                {
                                    "path": [
                                        "/ian1990/ddd"
                                    ]
                                }
                            ],
                            "handle": [
                                {
                                    "handler": "static_response",
                                    "body": "Acceso permitido a /ian1990/ddd (Gestionado por Cloud Guardian)"
                                }
                            ]
                        },
                        {
                            "match": [
                                {
                                    "path": [
                                        "/nube/trabajo"
                                    ]
                                }
                            ],
                            "handle": [
                                {
                                    "handler": "static_response",
                                    "body": "Acceso permitido a /nube/trabajo"
                                }
                            ]
                        },
                        {
                            "handle": [
                                {
                                    "handler": "reverse_proxy",
                                    "upstreams": [
                                        {
                                            "dial": ":8000"
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            }
        }
    }
}